{
  "name": "posthtml-webp",
  "description": "Add webp supporting in your html",
  "version": "1.0.3",
  "author": "seokirill",
  "ava": {
    "verbose": "true"
  },
  "bugs": "https://github.com/posthtml/posthtml-webp/issues",
  "dependencies": {
    "posthtml": "^0.11.4"
  },
  "devDependencies": {
    "ava": "^2.2.0",
    "conventional-changelog": "^3.1.10",
    "conventional-changelog-lint": "^2.1.1",
    "coveralls": "^3.0.5",
    "husky": "^3.0.2",
    "mversion": "^1.13.0",
    "nyc": "^14.1.1",
    "snazzy": "^8.0.0",
    "standard": "^13.1.0"
  },
  "engines": {
    "node": ">=8"
  },
  "homepage": "https://github.com/posthtml/posthtml-webp",
  "keywords": [
    "html",
    "posthtml",
    "posthtml-plugin",
    "webp"
  ],
  "license": "MIT",
  "main": "lib",
  "repository": "posthtml/posthtml-webp",
  "scripts": {
    "commitmsg": "conventional-changelog-lint -p angular -e",
    "lint": "standard | snazzy",
    "precommit": "npm run lint",
    "release-major": "mversion major",
    "release-minor": "mversion minor",
    "release-patch": "mversion patch",
    "test": "npm run lint && ava"
  }
}
